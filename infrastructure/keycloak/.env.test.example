# Keycloak Test Environment Variables
# Used by integration tests with docker-compose.keycloak-test.yml

# Keycloak Admin Console
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Keycloak HTTP Port (internal container)
KEYCLOAK_HTTP_PORT=8080

# Keycloak External Port (host machine)
# Using 8091 for test environment to avoid conflicts
KEYCLOAK_PORT=8091

# Redis Configuration for Tests
REDIS_PORT=6381
REDIS_PASSWORD=test-redis-password
